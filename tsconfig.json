{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 16",
  "_version": "16.1.0",
  "include": ["**/*"],
  "exclude": [ 
    "tests*",
    "playwright*"
  ],
  "compilerOptions": {
    "module": "Node16",
    "target": "es2021",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node16",
    "resolveJsonModule": true,
    "outDir": "dist"
  }
}
